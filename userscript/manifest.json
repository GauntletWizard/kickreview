{
  "name": "Kickbacker",
  "version": "1",
  "manifest_version": 2,
  "description": "Generate stats on Kickstarter projects backed",
  "permissions": [
    "http://www.kickstarter.com/"
  ],
  "content_scripts" : [
    {
      "matches" : ["http://www.kickstarter.com/profile/transactions*"],
      "js" : [ "kickback.js" ],
      "run_at" : "document_idle",
      "all_frames" : false,
//      "css": [ ]
    }
  ]
}

